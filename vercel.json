{
  "framework": "vite",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test.ts"
    },
    {
      "source": "/api/debug-app",
      "destination": "/api/debug-app.ts"
    },
    {
      "source": "/api/debug-db",
      "destination": "/api/debug-db.ts"
    },
    {
      "source": "/api/debug-minimal-pg",
      "destination": "/api/debug-minimal-pg.ts"
    },
    {
      "source": "/api/debug-pg-import",
      "destination": "/api/debug-pg-import.ts"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}